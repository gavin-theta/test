{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "StartUp": {
    "StyleSources": "stackpath.bootstrapcdn.com,cdnjs.cloudflare.com,cdn.jsdelivr.net",
    "ScriptSources": "code.jquery.com,cdnjs.cloudflare.com,stackpath.bootstrapcdn.com,cdnjs.cloudflare.com,cdn.jsdelivr.net",
    "ImageSources": "data:,https:",
    "DefaultSources": "data:,https:",
    "AllowedRedirectDestinations": "https://login.microsoftonline.com,https://asvcwelmarketdispatchdev1.azurewebsites.net,https://asvcwelmarketdispatchtest1.azurewebsites.net,https://asvcwelmarketdispatchprod1.azurewebsites.net"
  },
  "NotificationThreshold": {
    "Warn": 45,
    "Error": 60,
    "GroupCode": "IL"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "welmarketdispatch.onmicrosoft.com",
    "CallbackPath": "/signin-oidc"
  },
  "AllowedHosts": "*",
  "HangfireHeartbeatExpression": "* * * * *",
  "HangfireEnabled": true,
  "HangfireUIRoute": "/hangfire",
  "HangfireRebuildJobs": true,
  "ApplicationName": "BidWel Market Dispatch Portal",
  "MaxInactiveTime": 120,
  "MaxInactiveWarningTime": 115
}
